        </parameter>

        <parameter>
            <name>T_bc_right</name>
            <type>Constant</type>
            <value>313.15</value>
        </parameter>


        <parameter>
            <name>dirichlet0</name>
            <type>Constant</type>
            <value>0</value>
        </parameter>

<!-- source terms ###########################################################-->

        <parameter>
            <name>neumann</name>
            <type>Constant</type>
            <value>-4.e6</value>
        </parameter>



